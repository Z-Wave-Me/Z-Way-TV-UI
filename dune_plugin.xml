<dune_plugin>
    <name>z_wave_tv_ui</name>
    <caption>Z-Wave TV UI</caption>
    <entry_points>
        <entry_point>
            <parent_media_url>root://zwtv</parent_media_url>
            <media_url>z_wave_tv_ui</media_url>
            <caption>Z-Wave TV UI</caption>
            <icon_url>plugin_file://icon.png</icon_url>
            <actions>
                <key_enter>
                    <type>launch_media_url</type>
                    <data>
                        <url>www://http://your-http-server/tv-ui/:::fullscreen=1&amp;webapp_keys=1&amp;zoom_level=100&amp;osd_size=1280x720&amp;overscan=0&amp;background_color=black&amp;input_handler=0</url>
                    </data>
                </key_enter>
            </actions>
            <ip_address_required>yes</ip_address_required>
            <show_by_default>yes</show_by_default>
        </entry_point>
    </entry_points>
    <type>php</type>
</dune_plugin>